server:
  port: 9090

spring:
  redis:
    host: localhost
    port: 6379
  servlet:
    multipart:
      max-file-size: -1
      max-request-size: -1
  datasource:
    driver-class-name: com.mysql.cj.jdbc.Driver
    url: jdbc:mysql://localhost:3306/studyenglishdb?serverTimezone=GMT%2b8
    username: your_db_username        # change here
    password: your_db_password        # change here

mybatis:
  mapper-locations:
    classpath:mapper/*.xml

mybatis-plus:
  configuration:
    log-impl: org.apache.ibatis.logging.stdout.StdOutImpl